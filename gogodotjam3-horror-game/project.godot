[Subtitles]

General/subtitles_enabled=true
General/use_auto_dialogue_line_splitter=true
Advanced/auto_line_splitter_regular_expression="[^.|,|!|?|\"|'|:|;|`|ยบ|,|\\s]+"

[application]

config/name="GoGodotJam3 Horror Game"
run/main_scene="res://GameDefinitions/Levels/TestingLevel.tscn"
config/icon="res://icon.png"

[autoload]

Subtitles="*res://addons/subtitles/scenes/Subtitles.gd"
Globals="*res://Globals/autoload/Globals.gd"
LocksManager="*res://Globals/autoload/LocksManager.gd"
SaveData="*res://Globals/autoload/SaveData.gd"
SceneManagement="*res://Globals/autoload/SceneManagement.gd"
Settings="*res://Globals/autoload/Settings.gd"
Readables="*res://Globals/autoload/Readables.gd"

[editor]

scene/scene_naming=1

[editor_plugins]

enabled=PoolStringArray( "res://addons/color_grading_lut/plugin.cfg", "res://addons/jamcountdown/plugin.cfg", "res://addons/scatter-master/plugin.cfg", "res://addons/subtitles/plugin.cfg" )

